{
    "name": "furkifor/laravel_logging_url",
    "description": "test",
    "keywords": [
        "FurkiFor",
        "laravel_logging_url"
    ],
    "homepage": "https://github.com/furkifor/laravel_logging_url",
    "license": "MIT",
    "authors": [
        {
            "name": "furkan",
            "email": "furkanunsal69@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.4|^8.0"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^2.17",
        "phpunit/phpunit": "^9.5",
        "spatie/ray": "^1.10",
        "vimeo/psalm": "^4.3"
    },
    "autoload": {
        "psr-4": {
            "Furkifor\\LaravelLoggingUrl\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Furkifor\\LaravelLoggingUrl\\Tests\\": "tests"
        }
    },
    "scripts": {
        "psalm": "vendor/bin/psalm",
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage",
        "format": "vendor/bin/php-cs-fixer fix --allow-risky=yes"
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
